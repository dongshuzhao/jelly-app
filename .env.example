# Jelly Music App Configuration
# Copy this file to .env and customize the values for your setup

# The external port to expose the application on
JELLYAPP_PORT=80

# The default Jellyfin server URL to connect to
JELLYAPP_DEFAULT_URL=https://demo.jellyfin.org/stable

# Whether to lock the Jellyfin URL (true/false)
# Set to true if you want to prevent users from changing the server URL
JELLYAPP_LOCK_URL=false
